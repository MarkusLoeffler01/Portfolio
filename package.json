{
  "name": "portfolio_new",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npx concurrently \"vite --host\" \"npm run tailwind:watch\"",
    "build": "tsc && npm run tailwind && vite build",
    "tailwind": "tailwindcss -i ./src/css/tailwind.css -o ./src/css/output.css",
    "tailwind:watch": "tailwindcss -i ./src/css/tailwind.css -o ./src/css/output.css --watch",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "analyze": "source-map-explorer 'dist/assets/*.js'",
    "test": "vitest",
    "test:ui": "vitest --ui --coverage",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "formik": "^2.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-material-ui-carousel": "^3.4.2",
    "react-router-dom": "^6.28.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.7",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "@vitest/coverage-istanbul": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.18",
    "fluid-tailwind": "^1.0.4",
    "jsdom": "^25.0.1",
    "postcss": "^8.5.1",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.3",
    "vite": "^5.4.11",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.8"
  }
}
